<%= simple_form_for @political_party, html: { autocomplete: "off"}, url: @political_party.persisted? ? settings_political_party_path(id: @political_party.id) : settings_political_parties_path do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  
  <%= link_to 'Cancel', settings_path, class: 'btn btn-outline-secondary' %>
  <%= f.submit "Save", class: "btn btn-outline-success float-right", data: { disable_with: "Processing..." } %>
<% end %>